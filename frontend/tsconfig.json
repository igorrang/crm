{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      
      "@/components/*": ["src/components/*"]
    }
  },
  "include": ["../config/next-env.d.tsnv.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "../../next-env.d.ts"],
  "exclude": ["node_modules"]
}
